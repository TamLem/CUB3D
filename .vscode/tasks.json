{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/src/*.c",
                "${workspaceFolder}/src/parser/*.c",
                "${workspaceFolder}/src/utils/*.c",
                "${workspaceFolder}/inc/libft/libft.a",
                "${workspaceFolder}/_MLX42/libmlx42.a",
                "-lglfw",
                "-L/Users/tlemma/goinfre/.brew/opt/glfw/lib/",
                "-o",
                "${workspaceFolder}/a.out"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}